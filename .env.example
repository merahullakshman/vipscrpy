# Google Sheets API Configuration
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com

# Google Service Account Credentials (JSON string)
# Get this from Google Cloud Console > Service Accounts > Create Key
GOOGLE_CREDENTIALS={"type":"service_account","project_id":"..."}

# Application Settings
PORT=3000
NODE_ENV=development

# Scraping Settings
MAX_CONCURRENT_SCRAPES=3
REQUEST_DELAY_MS=1000
TIMEOUT_MS=30000

# Proxy Settings (optional - leave empty to not use proxy)
# Format: http://username:password@proxy-server:port or http://proxy-server:port
PROXY_URL=
# Comma-separated list of proxy URLs for rotation
PROXY_LIST=
